<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.MyController">
   <children>
      <Button fx:id="myButton" layoutX="20.0" layoutY="22.0" mnemonicParsing="false" onAction="#show" text="Set speed" />
      <TextField fx:id="myTextField" layoutX="20.0" layoutY="56.0" prefHeight="21.0" prefWidth="161.0" text="10" />
      <Canvas fx:id="myCanvas" height="200.0" layoutX="56.0" layoutY="131.0" width="200.0" />
      <Text fx:id="myText" layoutX="20.0" layoutY="105.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
      <Button fx:id="restartBtn" layoutX="110.0" layoutY="22.0" mnemonicParsing="false" onAction="#restarGame" text="Restart" />
   </children>
</AnchorPane>
